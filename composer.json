{
    "name": "ronei-kunkel/webstract",
    "description": "The web abstraction package",
    "version": "1.0.0-dev",
    "type": "library",
    "require-dev": {
        "pestphp/pest": "^3.8",
        "mockery/mockery": "^1.6",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "twig/twig": "^3.20"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RoneiKunkel\\Webstract\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\RoneiKunkel\\Webstract\\": "test/",
            "Test\\Support\\RoneiKunkel\\Webstract\\": "test/support/"
        }
    },
    "authors": [
        {
            "name": "Ronei Kunkel",
            "email": "ronei.kunkel@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --test-directory test --ci"
    }
}
